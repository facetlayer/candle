{
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src/**/*.ts",
    "src/*.ts"
, "src/main-wrappedSubprocess.ts.disabled", "src/startWrappedSubprocess.ts.disabled", "src/daemon/handleMessages.ts.disable", "src/daemon/messages.ts.disable"  ]
}